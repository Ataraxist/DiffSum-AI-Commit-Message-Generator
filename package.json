{
  "name": "diffsum",
  "version": "1.0.1",
  "main": "lib/commitMessage.js",
  "type": "module",
  "bin": {
    "diffsum": "bin/prepare-commit-msg"
  },
  "scripts": {
    "postinstall": "node bin/setup.js && cp bin/prepare-commit-msg .git/hooks/prepare-commit-msg && chmod +x .git/hooks/prepare-commit-msg"
  },
  "keywords": [
    "git",
    "commit",
    "AI",
    "OpenAI",
    "Git Hooks"
  ],
  "author": "Christopher Antico",
  "license": "MIT",
  "description": "An AI-powered Git commit message generator",
  "dependencies": {
    "dotenv": "^16.0.0",
    "openai": "^4.0.0",
    "simple-git": "^3.0.0"
  }
}
